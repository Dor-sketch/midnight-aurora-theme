{
  "manifest_version": 3,
  "name": "Midnight Aurora New Tab",
  "version": "1.0",
  "description": "A beautiful new tab page with a midnight aurora theme, clock, date, search bar, and quick links.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "midnight-aurora-new-tab@dorpascal.com"
    }
  }
}